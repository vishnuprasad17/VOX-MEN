                </svg></a>

            <!-- cart -->
            <a href="/cart"><svg xmlns="http://www.w3.org/2000/svg" width="26" height="26" viewBox="0 0 26 26"
                    fill="none">
                    <path d="M1 6.33337H25V25H1V6.33337Z" stroke="black" stroke-width="2" stroke-miterlimit="10" />
                    <path d="M18.1426 10.3333V1H7.85693V10.3333" stroke="black" stroke-width="2"
                        stroke-miterlimit="10" />
                </svg>
                <% if(locals.user && cartCount>0){ %>
                    <span class="position-absolute top-0 start-100 translate-middle badge rounded-pill bg-warning">
                        <%=cartCount %>
                    </span>
                    <% } %>

            </a>

            <!-- profile -->
            <a href="/profile"><svg xmlns="http://www.w3.org/2000/svg" width="26" height="26" viewBox="0 0 26 26"
                    fill="none">
                    <path d="M25 25L20.5067 18.4546H5.50667L1 25" stroke="black" stroke-width="2"
