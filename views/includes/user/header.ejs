        <div id="mobile-menu-wrap"></div>
    </div>
    <!-- Offcanvas Menu End -->

    <!-- Header Section Begin -->
    <header class="header" >
        <div class="header__top">
            <div class="row ps-lg-5 d-flex justify-content-around">
                <div class="col-lg-6 col-md-6 ">
                    <div class="header__top__left ">
                        <!--<p>VOX-MEN</p>-->
                        <p>FREE SHIPPING FOR ALL ORDERS NOW</p> 
                    </div>
                </div>
                <div class="col-lg-6 col-md-6 ">
                    <div class="header__top__right">
                        <!--<div class="header__top__links">
                            <% if(locals.user) {%>
                                <form action="/logout" method="post">
                                    <button type="submit" style="padding-top: 1vh; background: none; border: none; padding: 0; color: rgb(255, 255, 255)
                                ">Logout</button>
                                </form>
                                <% } else{%>
                                    <a href="/login">login</a>
                                    <% } %>

                        </div>-->
                        <div class="header__top__links">
                            <a href="/contact" style=" background: none;
                            border: none;
                            padding: 0;
                            color: rgb(255, 255, 255);
                            ">Contact</a>
                        </div>
                        <div class="header__top__links">
                            <a href="/about" style=" background: none;
                            border: none;
                            padding: 0;
                            color: rgb(255, 255, 255);
                            ">About</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!-- main head -->
        <div class="container-lg container-fluid">
            <div class="row">
                <div class="col-lg-2 col-md-2 search-fold-div">
                    <div class="pe-3 pt-4 pb-3">
                        <a href="/"><img src="/logo.png" alt="" id="vox-logo"></a>
                    </div>
                </div>
                <div class="col-lg-5 col-md-5 ">
                    <nav class="header__menu mobile-menu">
                        <ul class="">
                            <li><a id="navBar-Home" href="/">Home</a></li>
                            <li><a href="/shop?page=1">Shop</a></li>
                        </ul>
                    </nav>
                </div>
                <div class="d-md-none search-small-div">

                    <input type="search" placeholder="&nbsp;search" id="secondQueryInput" class="search-small"
                        style="width: 10rem; height: 2rem;" onkeydown="enterKey(event)">

                </div>

                <div class="col-lg-5 col-md-5">
                    <div class="header__nav__option">
                        <!-- search -->
                        <input type="search" onkeydown="checkEnterKey(event)" id="queryInput" placeholder="&nbsp;search"
                            class="" style="width: 12rem; height: 2rem;">
                        <button onclick="submitQuery()" class="me-4"
                            style="border: 1px solid black; background-color: black;"><i class="bi bi-search text-white"
                                style="background-color: black;"></i></button>


                        <custom-tag class="navBar-icons">
                            <a href="/wishList"><svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" fill="white" class="bi bi-heart" viewBox="0 0 16 16">
                                <path d="m8 2.748-.717-.737C5.6.281 2.514.878 1.4 3.053c-.523 1.023-.641 2.5.314 4.385.92 1.815 2.834 3.989 6.286 6.357 3.452-2.368 5.365-4.542 6.286-6.357.955-1.886.838-3.362.314-4.385C13.486.878 10.4.28 8.717 2.01L8 2.748zM8 15C-7.333 4.868 3.279-3.04 7.824 1.143c.06.055.119.112.176.171a3.12 3.12 0 0 1 .176-.17C12.72-3.042 23.333 4.867 8 15z"/>
                              </svg></a>

                            <a href="/cart"> <svg width="100%" viewBox="0 0 24 24" width="26" height="26">
    
                                <path fill="none" stroke="white" stroke-linecap="round" stroke-linejoin="round" stroke-width="1.25" class="cls-1" d="m19.84 7.53 1.4 12.88a1.73 1.73 0 0 1-1.53 1.91H4.48a1.73 1.73 0 0 1-1.73-1.72 1.15 1.15 0 0 1 0-.19L4.16 7.53a.27.27 0 0 1 .28-.25h15.12a.27.27 0 0 1 .28.25z"></path>
                                <path fill="none" stroke="white" stroke-linecap="round" stroke-linejoin="round" stroke-width="1.25" class="cls-1" d="M16.23 7.28V5.75a4.23 4.23 0 0 0-8.46 0v1.53"></path></svg>
                                <% if(locals.user && cartCount>0){ %>
                                    <span
                                        class="position-absolute top-0 start-100 translate-middle badge rounded-pill bg-warning">
                                        <%=cartCount %>
                                    </span>
                                    <% } %>

                            </a>
                            <a href="/profile"><svg width="100%" viewBox="0 0 24 24" width="26" height="26">
    
                                <circle fill="none" stroke="white" stroke-linecap="round" stroke-linejoin="round" stroke-width="1.25" cx="12" cy="6.47" r="4.8"></circle>
                                <path fill="none" stroke="white" stroke-linecap="round" stroke-linejoin="round" stroke-width="1.25" d="M21 22.33a.33.33 0 0 0 .32-.36c-.08-.76-.27-2.57-.37-3.28a4.54 4.54 0 0 0-4.51-3.92h-9A4.54 4.54 0 0 0 3 18.69c-.1.71-.29 2.52-.37 3.28a.33.33 0 0 0 .32.36z"></path>
                        
                              
                          </svg></a>
                        </custom-tag>
                    </div>
                </div>
            </div>
            <div class="canvas__open"><i class="bi bi-list" style="font-weight: bold;"></i></div>
        </div>
    </header>
    <hr class="header-line">
</section>