                    <form action="/logout" method="post">
                        <button type="submit" style=" background: none;
                    border: none;
                    padding: 0;
                    ">LOG OUT</button>
                    </form>
                    <% } else{%>
                        <a href="/login">login</a>
                        <% } %>

            </div>
            <div class="offcanvas__links" style=" background: none;
            border: none;
            padding: 0;
            ">
