<script>
    //main search tag
    function checkEnterKey(event) {
        if (event.key === 'Enter' || event.keyCode === 13) {
            submitQuery();
        }
    }
    function submitQuery() {
        const query = document.getElementById('queryInput').value;
        const baseUrl = '/shop'; // Replace with your desired link URL
        const urlWithQuery = `${baseUrl}?search=${encodeURIComponent(query)}`;

        // Open the link in the same window
        window.location.href = urlWithQuery;

        // If you want to open the link in a new tab, use the following line instead
        // window.open(urlWithQuery, '_blank');
    }

    //second search tag
    function enterKey(event) {
        if (event.key === 'Enter' || event.keyCode === 13) {
            searchQuerySubmit();
        }
    }
    function searchQuerySubmit() {
